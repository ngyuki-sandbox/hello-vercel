{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        { "source": "/api/hello/:hello", "destination": "/api/hello" },
        { "source": "/app/(.*)", "destination": "/api/index" }
    ],
    "functions": {
        "api/dummy.ts": {
            "includeFiles": "api/dummy*.txt"
        }
    }
}
